<div class="hero">
    <div class="flex-wrap">
      <div class="text">
      <h1>Discover New Poems. Share Your Work Anonymously.</h1>
      <h1>Art For Art's Sake</h1>
      </div>
      <div class="skull">
        <img src="/images/107054-ON95PC-369.jpg" alt="">
      </div>
    </div>
  </div>
<div class="poems">
  {{#each poems}}
    <div class="poem_card">
      <h2>{{this.title}}</h2>
      <p>{{this.text}}</p>
    </div>
  {{/each}}
</div>